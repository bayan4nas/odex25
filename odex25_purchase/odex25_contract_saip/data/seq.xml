<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="tender_application_sequence" model="ir.sequence">
            <field name="name">CoC Contract Payment Sequence</field>
            <field name="code">line.contract.installment.coc</field>
            <field name="prefix">PY-COC-%(year)s/%(month)s/0000</field>
            <field name="company_id" eval="False"/>
            <field name="use_date_range">True</field>
            <field eval="1" name="number_increment"/>
        </record>      
    </data>

    <record id="seq_payment_no" model="ir.sequence">
        <field name="name">Payment Number</field>
        <field name="code">line.contract.installment</field>
        <field name="prefix"> </field>
        <field name="padding">6</field>
        <field eval="1" name="number_increment"/>
    </record>

</odoo>
