<odoo>
    <template id="report_employee_identification">
        <t t-call="web.html_container" t-translation="off">
            <t t-foreach="docs" t-as="doc">
                <t t-call="hr_base.hr_layout">
                    <div class="report_box">
                        <style type="text/css">
                            @charset "UTF-8";
                            @font-face {
                            font-family: 'Somar';
