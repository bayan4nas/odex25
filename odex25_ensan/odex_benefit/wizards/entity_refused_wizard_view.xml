                            <field name="refused_reason"/>
                        </group>

                        <footer>
                            <button name="final_refuse" type="object" string="Refused" class="oe_highlight"/>
                            or
                            <button special="cancel" string="Cancel"/>
                        </footer>
                    </sheet>
                </form>

            </field>
        </record>
