<odoo>
    <data>
        <!-- Governmental fees -->
        <record model="ir.ui.view" id="gv_fees_line_tree">
            <field name="name">gv.fees.line.tree</field>
            <field name="model">gv.fees.line</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="g_fees_type"/>
                    <field name="fees_type"/>
                    <field name="fees_amount"/>
                    <field name="state"/>
                </tree>
            </field>
        </record>
        <record id="gv_fees_line_form" model="ir.ui.view">
            <field name="name">gv.fees.line.form</field>
            <field name="model">gv.fees.line</field>
            <field name="arch" type="xml">
                <form string="Governmental fees">
                    <header>
                        <button name="action_accepted" type="object"
                                string="Accept" class="oe_highlight"
                                confirm="Are you sure you want to accept  ?"
                                states="draft"/>
                        <field name="state" widget="statusbar"/>
                    </header>
                    <sheet>
                        <group>
                            <group>
                                <field name="name"/>
                                <field name="benefit_id" invisible="1"/>
                                <field name="g_fees_type"/>
                                <!--                                <field name="is_accreditation"/>-->
                            </group>
                            <group>
                                <field name="fees_type"/>
                                <field name="fees_amount"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
