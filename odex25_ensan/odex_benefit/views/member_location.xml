<odoo>
    <data>
        <record id="member_location_form" model="ir.ui.view">
            <field name="name">member.location.form</field>
            <field name="model">member.location</field>
            <field name="arch" type="xml">
                <form string="Member Location">
                    <sheet>
                        <group>
                             <group>
                                <field name="name"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="member_location_tree" model="ir.ui.view">
            <field name="name">member.location.tree</field>
            <field name="model">member.location</field>
            <field name="arch" type="xml">
                <tree string="Member Location">
                    <field name="name"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>
