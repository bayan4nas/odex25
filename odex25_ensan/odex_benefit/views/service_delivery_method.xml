<odoo>

<record id="view_service_delivery_method_tree" model="ir.ui.view">
  <field name="name">service.delivery.method.tree</field>
  <field name="model">service.delivery.method</field>
  <field name="arch" type="xml">
    <tree>
        <field name="name"/>
    </tree>
  </field>
</record>

<record id="view_service_delivery_method_form" model="ir.ui.view">
  <field name="name">service.delivery.method.form</field>
  <field name="model">service.delivery.method</field>
  <field name="arch" type="xml">
    <form string="Service Delivery Method">
        <sheet>
            <group>
                <field name="name"/>
            </group>
        </sheet>
    </form>
  </field>
</record>

    <record id="action_service_delivery_method_list" model="ir.actions.act_window">
  <field name="name">Service Delivery Method</field>
  <field name="res_model">service.delivery.method</field>
  <field name="view_mode">tree,form</field>
</record>


</odoo>