<odoo>

<record id="view_service_need_status_tree" model="ir.ui.view">
  <field name="name">service.need.status.tree</field>
  <field name="model">service.need.status</field>
  <field name="arch" type="xml">
    <tree>
        <field name="name"/>
    </tree>
  </field>
</record>

<record id="view_service_need_status_form" model="ir.ui.view">
  <field name="name">service.need.status.form</field>
  <field name="model">service.need.status</field>
  <field name="arch" type="xml">
    <form string="Service Need Status">
        <sheet>
            <group>
                <field name="name"/>
            </group>
        </sheet>
    </form>
  </field>
</record>

    <record id="action_service_need_status_list" model="ir.actions.act_window">
  <field name="name">Service Need Status</field>
  <field name="res_model">service.need.status</field>
  <field name="view_mode">tree,form</field>
</record>


</odoo>