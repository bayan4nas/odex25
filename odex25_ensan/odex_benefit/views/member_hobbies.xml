<odoo>
    <data>
        <record id="member_hobbies_form" model="ir.ui.view">
            <field name="name">member.hobbies.form</field>
            <field name="model">member.hobbies</field>
            <field name="arch" type="xml">
                <form string="Member Hobbies">
                    <sheet>
                        <group>
                             <group>
                                <field name="name"/>
                                <field name="member_id" invisible="1"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="member_hobbies_tree" model="ir.ui.view">
            <field name="name">member.hobbies.tree</field>
            <field name="model">member.hobbies</field>
            <field name="arch" type="xml">
                <tree string="Member Hobbies">
                    <field name="name"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>
