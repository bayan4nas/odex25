                                                    <option selected="selected" value="">
                                                        لايوجد </option>
                                                    <option value="general">
                                                        عامة</option>
                                                    <option value="especially">خاصة </option>
                                                </select>
                                            </div>
                                        </div>
                                        <!-- debt expenses part -->
                                        <div class="form-group debt_expenses col-lg-6">
                                            <label for="debt_reason" class="control-label"> سبب
                                                الدين </label>
