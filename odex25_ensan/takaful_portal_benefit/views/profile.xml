                                            <div class="attach_wrapper">
                                                <input type="file" class="form-control"
                                                    id="iban_file"
                                                    name="" />
                                                <input type="text" id="iban_file_preview" />
                                                <img
                                                    src="/takaful_portal_benefit/static/img/attatch.png"
                                                    alt="attach" />
                                            </div>
                                        </div>
                                        <!-- <div class="form-group col-lg-6">
                                            <label for="" class=" control-label"> إرفاق مستند رقم
                                                العائلة </label>
                                            <div class="attach_wrapper">
                                                <input type="file" class="form-control" id=""
                                                    name="" />
                                                <input type="text" />
                                                <img
                                                    src="/takaful_portal_benefit/static/img/attatch.png"
                                                    alt="attach" />
                                            </div>
                                        </div>
                                        <div class="form-group col-lg-6">
                                            <label for="" class=" control-label">إرفاق مستند اثبات
                                                الحالة </label>
                                            <div class="attach_wrapper">
                                                <input type="file" class="form-control" id=""
                                                    name="" />
                                                <input type="text" />
                                                <img
                                                    src="/takaful_portal_benefit/static/img/attatch.png"
                                                    alt="attach" />
                                            </div>
                                        </div> -->
                                        <div class="col-lg-12">
                                            <button class="main_link attach_btn"> حفظ البيانات </button>
                                        </div>
                                    </div>
                                </form>
                            </div>
                            <div id="benefits_data" class="tab-pane fade">
                                <span class="add_bnf" data-toggle="modal" data-target="#add_benefit">
                                    <img
                                        src="/takaful_portal_benefit/static/img/Icon awesome-plus.png"
                                        alt="plus" class="ml-2" /> أضف معال جديد </span>
                                <section class="">
                                    <table id="table_id" class="display w-100 followers_tbl">
                                        <thead>
                                            <tr>
                                                <th>الرقم</th>
                                                <th>اسم المعال</th>
                                                <th>الجنس</th>
                                                <th>العلاقة </th>
                                                <th> تاريخ الميلاد </th>
                                                <th> رقم الهوية </th>
                                                <th> ملف اثبات الهوية </th>
                                                <th style="padding:24px ;"> حذف </th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                        </tbody>
                                    </table>
                                </section>
                            </div>
                            <div id="pays_data" class="tab-pane fade">
                                <section class="">
                                    <table id="table_id" class="display w-100">
                                        <thead>
                                            <tr>
                                                <th>الاسم </th>
                                                <th>القيمة </th>
                                                <th>الحالة </th>
                                                <th class="date_cell">التاريخ</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                        
                                        </tbody>
                                    </table>
                                </section>
                            </div>
                            <div id="expenses" class="tab-pane fade">
                                <span class="add_bnf" data-toggle="modal" data-target="#add_expense">
                                    <img
                                        src="/takaful_portal_benefit/static/img/Icon awesome-plus.png"
                                        alt="plus" class="ml-2" /> أضف مصروف جديد </span>
                                <section class="">
                                    <table id="table_id" class="display w-100 expenses_table">
                                        <thead>
                                            <tr>
                                                <th>الرقم</th>
                                                <th>الاسم</th>
                                                <th> المصروفات </th>
                                                <th> نوع الرسوم </th>
                                                <th>المبلغ </th>
                                                <th> الدواء </th>
                                                <th> المرض </th>
                                                <th> المواصلات</th>
                                                <th> الدين</th>
                                                <th> سبب الدين</th>
                                                <th> شرح الجائحة</th>
                                                <th> الحالة </th>
                                                <th style="padding:24px ;"> المرفقات </th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                        </tbody>
                                    </table>
                                </section>
                            </div>

                        </div>
                    </div>
                </div>
                <!-- add benefit modal  -->
                <div class="modal fade kafala_modal " id="add_benefit" tabindex="-1" role="dialog"
                    aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
                    <div class="modal-dialog modal-dialog-centered" role="document">
                        <div class="modal-content">
                            <div class="modal-header">
                                <h5 class="modal-title heading" id="exampleModalLongTitle"> إضافه
                                    معال جديد </h5>
                                <button type="button" class="close" data-dismiss="modal"
                                    aria-label="Close">
                                    <span aria-hidden="true">
                                        <i class="fa fa-times"></i>
                                    </span>
                                </button>
                            </div>
                            <div class="modal-body">
                                <form id="add_support" action="/benefit/create_support"
                                    method="post">
                                    <div class="row">
                                        <input type="hidden" name="csrf_token"
                                            t-att-value="request.csrf_token()" />
                                        <div class="form-group col-lg-12">
                                            <label for="name" class="myName control-label"> الاسم </label>
                                            <div class="">
                                                <input type="text" class="form-control"
                                                    id="name" name="name" />
                                            </div>
                                        </div>
                                        <div class="form-group col-lg-12">
                                            <label for="relation" class="myName control-label">
                                                العلاقة</label>
                                            <div class="">
                                                <select name="relation" id="relation"
                                                    class="custom-select">
                                                    <option selected="selected" value="father">أب</option>
                                                    <option value="mother">أم </option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-lg-6 col-md-6">
                                            <div class="form-group">
                                                <label for="gender">الجنس</label>
                                                <div class="gender d-flex">
                                                    <div class="mail d-flex align-items-center">
                                                        <input type="radio"
                                                            class="d-inline-block w-auto"
                                                            id="gender-male" name="gender"
                                                            value="male" />
                                                        <label class="mb-0"> ذكر </label>
                                                    </div>
                                                    <div class="femail d-flex align-items-center">
                                                        <input type="radio"
                                                            class="d-inline-block w-auto"
                                                            id="gender-female" name="gender"
                                                            value="female" />
                                                        <label class="mb-0"> انثى </label>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group col-lg-12">
                                            <label for="birth_date" class="myName control-label">
                                                تاريخ الميلاد </label>
                                            <div class="pick_date position-relative">
                                                <input type="text" class="form-control"
                                                    id="birth" name="birth" />
                                                <input class="date_input" type="date"
                                                    name="birth_date" id="birth_date" />
                                                <img
                                                    src="/takaful_portal_benefit/static/img/Icon awesome-calendar-alt.png"
                                                    alt="calender" />
                                            </div>
                                        </div>
                                        <div class="form-group col-lg-12">
                                            <label for="id_number" class="myName control-label"> رقم
                                                الهوية </label>
                                            <div class="">
                                                <input type="text" class="form-control"
                                                    id="id_number" name="id_number" />
                                            </div>
                                        </div>
                                        <div class="form-group col-lg-6">
                                            <label for="id_number_attach"
                                                class="myName control-label">
                                                ارفاق الهوية </label>
                                            <div class="">
                                                <input type="file" class="form-control"
                                                    id="id_number_attach"
                                                    accept="application/msword, application/vnd.ms-excel, application/vnd.ms-powerpoint,
                                                    text/plain, application/pdf, image/*"
                                                    required="required"
                                                    name="id_number_attach" />
                                            </div>
                                        </div>
                                        <div class="form-group col-lg-6">
                                            <label for="father_name" class="myName control-label">
                                                اسم الأب </label>
                                            <div class="">
                                                <input type="text" class="form-control"
                                                    id="father_name" name="father_name" />
                                            </div>
                                        </div>

                                        <div class="form-group col-lg-6">
                                            <label for="father_id_number"
                                                class="myName control-label">رقم
                                                هوية الأب </label>
                                            <div class="">
                                                <input type="text" class="form-control"
                                                    id="father_id_number" name="father_id_number" />
                                            </div>
                                        </div>
                                        <div class="form-group col-lg-6">
                                            <label for="mother_name" class="myName control-label">
                                                اسم الأم </label>
                                            <div class="">
                                                <input type="text" class="form-control"
                                                    id="mother_name" name="mother_name" />
                                            </div>
                                        </div>
                                        <div class="form-group col-lg-6">
                                            <label for="mother_id_number"
                                                class="myName control-label">رقم
                                                هوية الأم </label>
                                            <div class="">
                                                <input type="text" class="form-control"
                                                    id="mother_id_number" name="mother_id_number" />
                                            </div>
                                        </div>
                                        <div class="form-group col-lg-6">
                                            <label for="father_is_life" class=" control-label"> حالة
                                                الأب </label>
                                            <select id="father_is_life" name="father_is_life"
                                                class="custom-select">
                                                <option selected="selected" value="true">على قيد
                                                    الحياة </option>

                                                <option value="false">متوفي</option>
                                            </select>
                                        </div>
                                        <div class="form-group col-lg-6">
                                            <label for="mother_is_life" class=" control-label"> حالة
                                                الأم </label>
                                            <select id="mother_is_life" name="mother_is_life"
                                                class="custom-select">
                                                <option selected="selected" value="true">على قيد
                                                    الحياة </option>
                                                <option value="false">متوفية</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="new_support"></div>
                                    <button type="submit" class="btn btn-primary"> اضافة </button>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- add expense modal  -->
                <div class="modal fade kafala_modal " id="add_expense" tabindex="-1" role="dialog"
                    aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
                    <div class="modal-dialog modal-dialog-centered" role="document">
                        <div class="modal-content">
                            <div class="modal-header">
                                <h5 class="modal-title heading" id="exampleModalLongTitle"> إضافه
                                    مصروف جديد </h5>
                                <button type="button" class="close" data-dismiss="modal"
                                    aria-label="Close">
                                    <span aria-hidden="true">
                                        <i class="fa fa-times"></i>
                                    </span>
                                </button>
                            </div>
                            <div class="modal-body">
                                <form id="expenses_form">
                                    <div class="row">
                                        <div class="form-group general_expenses col-lg-6">
                                            <label for="name" class=" control-label"> الاسم
                                            </label>
                                            <div class="name">
                                                <input type="text" name="name" id="name" />
                                            </div>
                                        </div>
                                        <div class="form-group general_expenses col-lg-6">
                                            <label for="expenses_type" class="control-label">
                                                نوع المصروف </label>
                                            <div class="d-flex add_wrapper ">
                                                <select style="margin-inline-end:8px"
                                                    name="expenses_type" id="expenses_type"
                                                    class="custom-select">
                                                    <option selected="selected" value="governmental">
                                                        مصروفات حكومية </option>
                                                    <option value="medical"> مصروفات علاجية </option>
                                                    <option value="transportation"> مصروفات
                                                        المواصلات </option>
                                                    <option value="debts"> مصروفات الديون </option>
                                                    <option value="pandemics"> مصروفات الجوائح </option>
                                                    <option value="living"> مصروفات المعيشة </option>
                                                    <option value="educational"> مصروفات التعليم </option>
                                                    <option value="clothing"> مصروفات الملابس </option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="form-group general_expenses col-lg-6">
                                            <label for="expenses_fees_type"> نوع الرسوم </label>
                                            <div class="">
                                                <select style="margin-inline-end:8px"
                                                    name="expenses_fees_type"
                                                    id="expenses_fees_type"
                                                    class="custom-select">
                                                    <option value="fixed"> ثابتة </option>
                                                    <option value="dynamic"> متغيرة </option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="form-group general_expenses col-lg-6">
                                            <label for="amount" class=" control-label"> قيمة
                                                الرسوم </label>
                                            <div class="fees_value">
                                                <input type="number" name="amount" id="amount" />
                                            </div>
                                        </div>
                                        <!-- medical expenses part  -->
                                        <div class="form-group medical_expenses col-lg-6">
                                            <label for="medicine_type" class="control-label">
                                                نوع الدواء </label>
                                            <select name="medicine_type" id="medicine_type"
                                                class="custom-select">
                                                <option selected="selected" value="">
                                                    لايوجد</option>
                                                <option value="pills">

                                                    حبوب</option>
                                                <option value="drink">شراب </option>
                                                <option value="inhalation">استنشاق </option>
                                            </select>
                                        </div>
                                        <div class="form-group medical_expenses col-lg-6">
                                            <label for="diseases_type" class="control-label">
                                                نوع المرض </label>
                                            <div class="">
                                                <select name="diseases_type" id="diseases_type"
                                                    class="custom-select">
                                                    <option selected="selected" value="">
                                                        لايوجد</option>
                                                    <option value="chronic">
                                                        مزمن</option>
                                                    <option value="psycho">نفسي </option>
                                                    <option value="organic">عضوي </option>
                                                </select>
                                            </div>
                                        </div>
                                        <!-- transport expenses part  -->
                                        <div class="form-group transport_expenses col-lg-6">
                                            <label for="trans_type">نوع التنقلات</label>
                                            <div class="">
                                                <select name="trans_type" id="trans_type"
